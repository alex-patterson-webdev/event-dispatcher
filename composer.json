{
  "name" : "alex-patterson-webdev/event-dispatcher",
  "version": "1.0.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "psr/event-dispatcher": "^1",
    "alex-patterson-webdev/factory": "^1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit" : "^8"
  },
  "autoload": {
    "psr-4": {
      "Arp\\EventDispatcher\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\EventDispatcher\\" : "test/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
